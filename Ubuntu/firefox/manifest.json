{
    "manifest_version": 2,
    "name": "Tab Focuser",
    "version": "1.0",
    "description": "Focuses the second tab upon clicking the extension icon",
    "permissions": [
        "tabs", 
        "http://localhost:5000/"
    ],
    "background": {
        "scripts": ["socket.io.min.js", "background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Click to focus tab"
    },
    "icons": {
        "48": "icon.png"
    }
}
